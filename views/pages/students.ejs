<% include ../partials/header %>
<h1> ALL STUDENT </h1>
    <table class="table">
        <thead>
            <tr>
                <th>Student ID</th>
                <th>Title</th>
                <th>Firstname</th>
                <th>Lastname</th>

            </tr>
        </thead>
        <tbody>
          <% students.forEach(function(students){ %>
        <tr>
            <td scope="row"><%= students.sid%></td>
            <td><%= students.title%></td>
            <td><%= students.firstname%></td>
            <td><%= students.lastname%></td>
        </tr>
        <% }); %>
        </tbody>
    </table>                                                                        


<% include ../partials/footer %>