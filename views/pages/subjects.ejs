<% include ../partials/header %>
<h1> SUBJECT </h1>
    <table class="table">
        <thead>
            <tr>
                <th>Code</th>
                <th>Title</th>
                <th>Credit</th>
                <th>Sections</th>

            </tr>
        </thead>
        <tbody>
        <% subjects.forEach(function(subjects){ %>
        <tr>
            <td scope="row"><%= subjects.code%></td>
            <td><%= subjects.title%></td>
            <td><%= subjects.credits%></td>
            <td><%= subjects.sections%></td>
        </tr>
        <% }); %>
        </tbody>
    </table>                                                                        


<% include ../partials/footer %>
  